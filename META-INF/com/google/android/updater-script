assert(getprop("ro.product.device") == "lcsh92_wet_tdd" ||
       getprop("ro.build.product") == "lcsh92_wet_tdd");
show_progress(0.300000, 0);
mount("ext4", "EMMC", "/dev/block/mmcblk0p6", "/system");
package_extract_file("system/build.prop","/system/build.prop");
package_extract_file("system/xbin/su","/system/xbin/su");
set_perm(0, 0, 06755, "/system/xbin/su");
show_progress(0.600000, 0);
unmount("/system");
show_progress(1.000000, 0);
ui_print("Done.");
